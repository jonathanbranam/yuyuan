<h1>Listing chinese_words</h1>

<table>
  <tr>
    <th>Traditional</th>
    <th>Simplified</th>
    <th>Pinyin</th>
    <th>English1</th>
    <th>English2</th>
    <th>English3</th>
    <th>English4</th>
    <th>English5</th>
    <th>English6</th>
    <th>English7</th>
    <th>English8</th>
    <th>English9</th>
  </tr>

<% @chinese_words.each do |chinese_word| %>
  <tr>
    <td><%=h chinese_word.traditional %></td>
    <td><%=h chinese_word.simplified %></td>
    <td><%=h chinese_word.pinyin %></td>
    <td><%=h chinese_word.english1 %></td>
    <td><%=h chinese_word.english2 %></td>
    <td><%=h chinese_word.english3 %></td>
    <td><%=h chinese_word.english4 %></td>
    <td><%=h chinese_word.english5 %></td>
    <td><%=h chinese_word.english6 %></td>
    <td><%=h chinese_word.english7 %></td>
    <td><%=h chinese_word.english8 %></td>
    <td><%=h chinese_word.english9 %></td>
    <td><%= link_to 'Show', chinese_word %></td>
    <td><%= link_to 'Edit', edit_chinese_word_path(chinese_word) %></td>
    <td><%= link_to 'Destroy', chinese_word, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New chinese_word', new_chinese_word_path %>